<!-- CAROUSEL -->
  .container.carouselsize
    #carouselExampleControls.carousel.slide(data-ride='carousel')
      .carousel-inner
        //- .carousel-item.active
        //-   img.d-block.w-100(src='../images/dragon-boat.jpg' alt='First slide')
        .carousel-item.active
          img.d-block.w-100(src='../images/greek.jpg' alt='Second slide')
        .carousel-item
          img.d-block.w-100(src='../images/happy-panda.jpg' alt='Third slide')
        .carousel-item
          img.d-block.w-100(src='../images/mumbai-bites.jpg' alt='Forth slide')
        //- .carousel-item
        //-   img.d-block.w-100(src='../images/thecurry.jpg' alt='Fifth slide')
      a.carousel-control-prev(href='#carouselExampleControls' role='button' data-slide='prev')
        span.carousel-control-prev-icon(aria-hidden='true')
        span.sr-only Previous
      a.carousel-control-next(href='#carouselExampleControls' role='button' data-slide='next')
        span.carousel-control-next-icon(aria-hidden='true')
        span.sr-only Next